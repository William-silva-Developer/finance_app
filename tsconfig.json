{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@src/*":["./src/*"],
      "@assets/*":["./assets/*"],
      "@components/*":["./components/*"],
      "@config/*":["./config/*"],
      "@hooks/*":["./hooks/*"],
      "@routes/*":["./routes/*"],
      "@screens/*":["./screens/*"],
      "@service/*":["./service/*"],
      "@styles/*":["./styles/*"],
      "@utils/*":["./utils/*"],
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
